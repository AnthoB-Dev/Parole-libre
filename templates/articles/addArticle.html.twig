{% extends 'base.html.twig' %}

{% block title %}Ajouter un article{% endblock %}

{% block body %}

   <h1>Ajouter un article</h1>

   <div>
      {{form_start(form)}}

         {{form_row(form.category)}}
         {{form_row(form.title)}}
         {{form_row(form.subtitle)}}
         {{form_row(form.image)}}
         {{form_row(form.content)}}

         <button type="submit">Ajouter un article</button>
      {{form_end(form)}}
   </div>

{% endblock %}
